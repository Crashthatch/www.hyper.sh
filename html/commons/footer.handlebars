<div class="form">
  <div class="left">
    <div class="title">Get Started</div>
    <ul class="start">
      <li class="item">
        <div class="circle">
          <span class="orange"><i class="fa fa-user"></i></span>
        </div>
        <div class="desc">Create Account</div>
      </li>
      <li class="item">
        <div class="circle">
          <span class="orange"><i class="fa fa-lock"></i></span>
        </div>
        <div class="desc">Generate API Credential</div>
      </li>
      <li class="item">
        <div class="circle">
          <span class="orange"><i class="fa fa-terminal"></i></span>
        </div>
        <div class="desc">Setup CLI on Your Laptop</div>
      </li>
    </ul>
  </div>
  <div class="fields">
    <form name="signup" action="https://console.hyper.sh/register" method="GET">
      <div class="wrap">
        <input type="text" name="firstname" class="firstname" placeholder="First Name" required>
        <input type="text" name="lastname" class="lastname" placeholder="Last Name" required>
      </div>
      <input type="email" name="email" class="email" placeholder="Email" required>
      <input type="password" name="password" class="password" placeholder="Password" minlength="8" maxlength="64" required>
      <input type="submit" class="signup" value="GO!" href="https://console.hyper.sh/register"></input>
    </form>
  </div>
</div>
